{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "drughunter/plans-table",
  "version": "0.1.0",
  "title": "Plans Table",
  "category": "widgets",
  "attributes": {
    "planHeadline": {
      "type": "string",
      "source": "html",
      "selector": "h3"
    },
    "planSubhead": {
      "type": "string"
    },
    "planPriceLine": {
      "type": "string"
    },
    "planPeriod": {
      "type": "string"
    },
    "planByline": {
      "type": "string"
    },
    "planButtonText": {
      "type": "string"
    }
  },
  "example": {
    "attributes": {
      "planHeadline": "QUARTERLY",
      "planSubhead": "free week, then",
      "planPriceLine": "$120",
      "planPeriod": "a month, billed quarterly"
    }
  },
  "icon": "money",
  "description": "Plans Table showing all packages available",
  "supports": {
    "html": false
  },
  "textdomain": "drughunter",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}